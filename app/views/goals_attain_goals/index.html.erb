<p id="notice"><%= notice %></p>

<h1>Goals Attain Goals</h1>

<table>
  <thead>
    <tr>
      <th>Validity</th>
      <th>Goal</th>
      <th>Actor</th>
      <th>Actor type</th>
      <th>Attainment</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goals_attain_goals.each do |goals_attain_goal| %>
      <tr>
        <td><%= goals_attain_goal.validity %></td>
        <td><%= goals_attain_goal.goal %></td>
        <td><%= goals_attain_goal.actor %></td>
        <td><%= goals_attain_goal.actor_type %></td>
        <td><%= goals_attain_goal.attainment %></td>
        <td><%= goals_attain_goal.start_date %></td>
        <td><%= goals_attain_goal.end_date %></td>
        <td><%= goals_attain_goal.score %></td>
        <td><%= link_to 'Show', goals_attain_goal %></td>
        <td><%= link_to 'Edit', edit_goals_attain_goal_path(goals_attain_goal) %></td>
        <td><%= link_to 'Destroy', goals_attain_goal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Goals Attain Goal', new_goals_attain_goal_path %>
