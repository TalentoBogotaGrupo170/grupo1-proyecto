<p id="notice"><%= notice %></p>

<h1>Healthpromentities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Mail</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @healthpromentities.each do |healthpromentity| %>
      <tr>
        <td><%= healthpromentity.name %></td>
        <td><%= healthpromentity.mail %></td>
        <td><%= healthpromentity.link %></td>
        <td><%= link_to 'Show', healthpromentity %></td>
        <td><%= link_to 'Edit', edit_healthpromentity_path(healthpromentity) %></td>
        <td><%= link_to 'Destroy', healthpromentity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Healthpromentity', new_healthpromentity_path %>
